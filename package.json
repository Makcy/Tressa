{
	"name": "tressa",
	"displayName": "Weather",
	"description": "Simple weather display plugin",
	"keywords": ["weather", "天气"],
	"version": "1.0.0",
	"publisher": "makcy",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:weather.refresh"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "weather.refresh",
				"title": "Weather: refresh"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"request": "^2.88.0",
		"request-promise": "^4.2.5"
	}
}
